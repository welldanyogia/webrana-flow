APP_NAME="Webrana Flow"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://panel.tbferdojayaabadi.shop

### LOCALIZATION
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

### DRIVERS
APP_MAINTENANCE_DRIVER=file
BCRYPT_ROUNDS=12
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

### DATABASE (Primary)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=webrana_flow
DB_USERNAME=root
DB_PASSWORD=

### SHARED DATABASE (For User Apps)
# Ini wajib ada di example agar developer tahu sistem ini butuh koneksi kedua
SHARED_DB_HOST=127.0.0.1
SHARED_DB_PORT=3306
SHARED_DB_USER=root
SHARED_DB_PASSWORD=

### SESSION & COOKIES (Crucial for Subdomains)
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
# Penting: Gunakan format domain root dengan titik di depan
SESSION_DOMAIN=.tbferdojayaabadi.shop

### QUEUE & CACHE
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database
CACHE_STORE=database

### REDIS (Optional/Future)
MEMCACHED_HOST=127.0.0.1
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

### MAIL
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="admin@webrana.id"
MAIL_FROM_NAME="${APP_NAME}"

### STORAGE (S3/R2)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

### VITE
VITE_APP_NAME="${APP_NAME}"

### FILAMENT PANELS (Subdomains)
# Ini config penting untuk memisahkan Admin dan User
FILAMENT_ADMIN_DOMAIN=panel.tbferdojayaabadi.shop
FILAMENT_USER_DOMAIN=console.tbferdojayaabadi.shop

### GITHUB OAUTH
# Kosongkan secretnya, tapi pertahankan key-nya
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=https://console.tbferdojayaabadi.shop/auth/github/callback
